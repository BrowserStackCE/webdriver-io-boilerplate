{
  "name": "wdio-mocha",
  "version": "1.0.0",
  "description": "WebdriverIO boilerplate using WebdriverIO, Mocha, and TypeScript",
  "scripts": {
    "test": "npm run build && wdio ./wdio.conf.js",
    "build": "tsc -p .",
    "prepare": "husky install"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@wdio/cli": "^7.6.1",
    "@wdio/local-runner": "^7.6.1",
    "@wdio/mocha-framework": "^7.6.0",
    "browserstack-local": "^1.4.8",
    "chai": "^4.3.4",
    "webdriverio": "^7.6.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.18",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.17.1",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown ."
  }
}
